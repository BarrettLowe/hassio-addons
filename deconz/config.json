{
    "name": "deCONZ",
    "version": "1.0",
    "slug": "deconz",
    "description": "Control a ZigBee network with Conbee or RaspBee by Dresden Elektronik",
    "arch": ["aarch64","amd64","armhf","i386"],
    "url": "https://home-assistant.io/addons/deconz",
    "startup": "system",
    "boot": "auto",
    "webui": "http://[HOST]:[PORT:8080]",
    "ports": {
        "80/tcp": 9880,
        "8080/tcp": 9881
    },
    "auto_uart": true,
    "apparmor": false,
    "options": {
        "deconz_device": null
    },
    "schema": {
        "deconz_device": "str"
    },
    "image": "homeassistant/{arch}-addon-deconz"
}
